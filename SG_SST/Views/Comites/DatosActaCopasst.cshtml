                            Fecha
                        </th>
                        <th style="border-right: 2px solid lightslategray; border-bottom: 2px solid lightslategray; text-align:center; vertical-align:middle; text-transform:uppercase">
                            Acción
                        </th>
                    </tr>
                    @{
                        if (Model.AccionesActaCopasst != null)
                        {
                            int fila = 1;
                            foreach (var item in Model.AccionesActaCopasst)
                            {

                                <tr class="trAcciones" id="trAcciones">
                                    <td style="border-right: 2px solid lightslategray; border-bottom: 2px solid lightslategray; text-align:center">
                                        @fila
                                    </td>
                                    <td style="border-right: 2px solid lightslategray; border-bottom: 2px solid lightslategray;">
                                        @Html.DisplayFor(modelItem => item.AccionARealizar)
                                    </td>
                                    <td style="border-right: 2px solid lightslategray; border-bottom: 2px solid lightslategray; ">
                                        @Html.DisplayFor(modelItem => item.Responsable)
                                    </td>
                                    <td style="border-right: 2px solid lightslategray; border-bottom: 2px solid lightslategray; text-align:center">
                                        @Html.DisplayFor(modelItem => item.FechaProbable, new { @value = item.FechaProbable.ToString("yyyy-MM-dd") })
                                    </td>
                                    <td style="border-right: 2px solid lightslategray; border-bottom: 2px solid lightslategray; vertical-align:middle; text-align:center">
                                        <button type="button" class="btn-search" data-toggle="modal" onclick="EliminarAccionCopasst(@string.Format("{0}", item.Fk_Id_Acta), @string.Format("{0}", item.Pk_Id_AccionActaCopasst))" title="Eliminar">
                                            <span class="glyphicon glyphicon-erase"></span>
                                        </button>
                                    </td>
                                </tr>
                                fila++;
                            }
                        }
                    }
                    <tbody id="tbAccion"></tbody>
                </table>
                <center><div class="pagination pagination4" id="paginadorAccion"></div></center>
            </div>
            <hr style="border-top-color:lightslategray" />

            <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <div class="col-md-12">
                            <input type="file" value="Cargar Documento" name="NombreArchivo" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <div class="col-md-10">
                            <button id="ContinuarMiembro" type="submit" class=" boton botoninfo">ADJUNTAR</button>
                            <label for="NombreArchivo" class="error" style="display:none"></label>
                       </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-3">
                    <div class="form-group">
                        <div class="col-md-5">
                            <a id="AtrasActa" href="@Url.Action("MiembroActaCopasstGuardados", new { PK_Id_Acta = Model.PK_Id_Acta, IdSede = @Model.Fk_Id_Sede, consecutivo_acta = Model.Consecutivo_Acta })" class="boton botonactive">ATRAS</a>
                        </div>
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <div class="col-md-5">
                            <button type="button" id="GrabarActa" onclick="ActualizarActaCopasst()" class="boton botoninfo">GUARDAR</button>
                        </div>
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <div class="col-md-5">
                            <a id="CancelarActa" href="@Url.Action("Index", "Comites")" class="boton botoncancel">CANCELAR</a>
                        </div>
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <div class="col-md-5">
                            <a id="Exportar" href="@Url.Action("ActaCopasst_PDF", new { PK_Id_Acta = @Model.PK_Id_Acta })" class=" boton botoninfo">EXPORTAR</a>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</form>
