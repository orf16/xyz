                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label class="col-md-12">RESPONSABLE:</label>
                        <div class="col-md-12">
                            @Html.TextBoxFor(model => Model.Responsable, new { @class = "form-control", @id = "Responsable", @name = "Responsable", @maxlength = "60" })
                            <label for="Responsable" class="error3" style="display:none"></label>
