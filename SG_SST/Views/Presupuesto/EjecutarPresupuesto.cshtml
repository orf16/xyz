    <input name="PK_Presupuesto_Por_Año" value="@ViewBag.PresupuestoPorAnio.PK_Presupuesto_Por_Año" type="hidden" />
    <input name="FK_Presupuesto" value="@ViewBag.PresupuestoPorAnio.FK_Presupuesto" type="hidden" />
    <input name="RubroTotal" id="RubroTotal" value="@ViewBag.Presupuesto.RubroTotal" type="hidden" />
    <input name="Periodo" id="Periodo" value="@ViewBag.PresupuestoPorAnio.Periodo" type="hidden" />
    <input name="FK_Sede" id="FK_Sede" value="@ViewBag.PresupuestoPorAnio.FK_Sede" type="hidden" />

    <input type="hidden" id="cantidadActividades" value="@Model.Count()" />
