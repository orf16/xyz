@section Scripts {
    @Scripts.Render("~/bundles/liderazgoGerencial")

    <script>
        $(document).ready(function () {
            $("#actividades").find("input[name*='PresupuestoEjecutadoPorMes']").each(function (index, elemento) {
                var numergo = parseFloat($(elemento).attr("data-content"));
                $(elemento).attr("data-content", darFormato(numergo.toFixed(2)));
                $(elemento).popover();
            });           
            $('#TablaPresupuesto').find("input[type=decimal]").on({
                "focus": function (event) {
                    $(event.target).select();
                },
                "keyup": function (event) {
                    $(event.target).val(function (index, value) {
                        valor = value.replace(/\D/g, "")
                                    .replace(/([0-9])([0-9]{2})$/, '$1,$2')
                                    .replace(/\B(?=(\d{3})+(?!\d)\.?)/g, ".");
                        stringValor = "$".concat(valor)
                        return stringValor;
                    });
                }
            });
            $("#RubroTotal").on({
                "focus": function (event) {
                    $(event.target).select();
                },
                "keyup": function (event) {
                    $(event.target).val(function (index, value) {
                        valor = value.replace(/\D/g, "")
                                    .replace(/([0-9])([0-9]{2})$/, '$1,$2')
                                    .replace(/\B(?=(\d{3})+(?!\d)\.?)/g, ".");
                        stringValor = "$".concat(valor)
                        return stringValor;
                    });
                }
            });
            sumarPresupuestoMes();
            //  $('#RubroTotal').val(darFormato($('#RubroTotal').val()));
            $('#TablaPresupuesto').find("input[type=decimal]").each(function (ind, element) {
                var numero = parseFloat($(element).val())
                $(element).val(darFormato(numero.toFixed(2)));
            });

            $('#TablaPresupuesto').find("input[name=total]").each(function (ind, element) {
                var numero = parseFloat($(element).val())
                $(element).val(darFormato(numero.toFixed(2)));
            });

        });

        //function titlePresupuestoPlaneado(elemento)
        //{
        //    $(elemento).popover();
        //}

    </script>
