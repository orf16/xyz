  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>db</DataSourceName>
   
        <CommandText>SELECT        Tbl_Empresa.Razon_Social AS razonsoc, Tbl_Sede.Nombre_Sede AS SEDE, Tbl_Proceso.Descripcion_Proceso AS PROCESO, 
                         Tbl_Proceso_1.Descripcion_Proceso AS SUBPROCESO, Tbl_Clasificacion_De_Peligro.Descripcion_Clase_De_Peligro AS PELIGRO_CLASIFICACION, 
                         Tbl_Tipo_De_Peligro.Descripcion_Del_Peligro AS PELIGRO_DESCRIPCIÓN, COUNT(Tbl_Persona_Expuesta_INSHT_RAM.Total) 
                         AS NUMERO_DE_EXPUESTOS_TOTAL, Tbl_RAM.Nivel_De_Riesgo
FROM            Tbl_Empresa INNER JOIN
                         Tbl_Peligro INNER JOIN
                         Tbl_Proceso ON Tbl_Peligro.FK_Proceso = Tbl_Proceso.Pk_Id_Proceso INNER JOIN
                         Tbl_Sede ON Tbl_Peligro.FK_Sede = Tbl_Sede.Pk_Id_Sede ON Tbl_Empresa.Pk_Id_Empresa = Tbl_Sede.Fk_Id_Empresa INNER JOIN
                         Tbl_Clasificacion_De_Peligro ON Tbl_Peligro.FK_Clasificacion_De_Peligro = Tbl_Clasificacion_De_Peligro.PK_Clasificacion_De_Peligro INNER JOIN
                         Tbl_Tipo_De_Peligro ON Tbl_Clasificacion_De_Peligro.FK_Tipo_De_Peligro = Tbl_Tipo_De_Peligro.PK_Tipo_De_Peligro INNER JOIN
                         Tbl_RAM INNER JOIN
                         Tbl_Persona_Expuesta_INSHT_RAM ON Tbl_RAM.FK_Persona_Expuesta = Tbl_Persona_Expuesta_INSHT_RAM.PK_Persona_Expuesta ON 
                         Tbl_Peligro.PK_Peligro = Tbl_Persona_Expuesta_INSHT_RAM.FK_Peligro LEFT OUTER JOIN
                         Tbl_Probabilidad INNER JOIN
                         Tbl_Consecuencia_Por_Peligro INNER JOIN
                         Tbl_Tipo_Metodologia INNER JOIN
                         Tbl_Consecuencia INNER JOIN
                         Tbl_Grupo ON Tbl_Consecuencia.FK_Grupo = Tbl_Grupo.PK_Grupo ON Tbl_Tipo_Metodologia.PK_Metodologia = Tbl_Grupo.FK_Metodologia ON 
                         Tbl_Consecuencia_Por_Peligro.FK_Consecuencia = Tbl_Consecuencia.PK_Consecuencia ON 
                         Tbl_Probabilidad.FK_Metodologia = Tbl_Tipo_Metodologia.PK_Metodologia ON 
                         Tbl_Peligro.PK_Peligro = Tbl_Consecuencia_Por_Peligro.FK_Peligro LEFT OUTER JOIN
                         Tbl_Proceso AS Tbl_Proceso_1 ON Tbl_Proceso.Fk_Id_Proceso = Tbl_Proceso_1.Pk_Id_Proceso LEFT OUTER JOIN
                         Tbl_Tipo_De_Peligro AS Tbl_Tipo_De_Peligro_1 ON Tbl_Clasificacion_De_Peligro.FK_Tipo_De_Peligro = Tbl_Tipo_De_Peligro_1.PK_Tipo_De_Peligro

GROUP BY Tbl_Proceso.Descripcion_Proceso, Tbl_Sede.Nombre_Sede, Tbl_Empresa.Razon_Social, Tbl_Proceso_1.Descripcion_Proceso, 
                         Tbl_Clasificacion_De_Peligro.Descripcion_Clase_De_Peligro, Tbl_Tipo_De_Peligro.Descripcion_Del_Peligro, Tbl_RAM.Nivel_De_Riesgo</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
