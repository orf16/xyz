  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>dbpositiva</DataSourceName>
     
        <CommandText>SELECT        Tbl_Empresa.Nit_Empresa, Tbl_Empresa.Razon_Social AS Razon_Social, Tbl_Modulos_Plan_Accion.Modulo, Tbl_Modulos_Plan_Accion.Actividad, 
                         SUM(CASE WHEN dbo.Tbl_Actividad_Plan_Accion.FechaCierre IS NOT NULL THEN 1 ELSE 0 END) AS Totalejecutadas_plan, Tbl_Acciones.Estado
FROM            Tbl_PlanAccionCorrectiva RIGHT OUTER JOIN
                         Tbl_PlanAccionInspeccion AS Tbl_PlanAccionInspeccion_1 ON 
                         Tbl_PlanAccionCorrectiva.Fk_Id_PlanAcccionInspeccion = Tbl_PlanAccionInspeccion_1.Pk_Id_PlanAcccionInspeccion LEFT OUTER JOIN
                         Tbl_PlanAccionporCondicion ON 
                         Tbl_PlanAccionInspeccion_1.Pk_Id_PlanAcccionInspeccion = Tbl_PlanAccionporCondicion.Fk_Id_PlanAcccionInspeccion RIGHT OUTER JOIN
                         Tbl_ActividadesActosInseguros INNER JOIN
                         Tbl_Actividad_Plan_Accion ON Tbl_ActividadesActosInseguros.PK_ID_ActividadActosInseguros = Tbl_Actividad_Plan_Accion.Fk_Id_Actividad RIGHT OUTER JOIN
                         Tbl_Modulos_Plan_Accion ON Tbl_Actividad_Plan_Accion.Fk_Id_ModuloPlanAccion = Tbl_Modulos_Plan_Accion.Pk_Id_ModuloPlanAccion ON 
                         Tbl_PlanAccionInspeccion_1.Pk_Id_PlanAcccionInspeccion = Tbl_Actividad_Plan_Accion.Fk_Plan_Inspección FULL OUTER JOIN
                         Tbl_Reportes INNER JOIN
                         Tbl_Sede ON Tbl_Reportes.FK_Sede = Tbl_Sede.Pk_Id_Sede INNER JOIN
                         Tbl_Empresa INNER JOIN
                         Tbl_Acciones INNER JOIN
                         Tbl_ActividadAccion ON Tbl_Acciones.Pk_Id_Accion = Tbl_ActividadAccion.Fk_Id_Accion ON Tbl_Empresa.Pk_Id_Empresa = Tbl_Acciones.Fk_Id_Empresa ON 
                         Tbl_Sede.Fk_Id_Empresa = Tbl_Empresa.Pk_Id_Empresa AND Tbl_Reportes.FK_NitEmpresa = Tbl_Empresa.Nit_Empresa LEFT OUTER JOIN
                         Tbl_ActasCopasst INNER JOIN
                         Tbl_AccionesActaCopasst ON Tbl_ActasCopasst.PK_Id_Acta = Tbl_AccionesActaCopasst.Fk_Id_Acta ON 
                         Tbl_Sede.Pk_Id_Sede = Tbl_ActasCopasst.Fk_Id_Sede FULL OUTER JOIN
                         Tbl_ActaRevision RIGHT OUTER JOIN
                         Tbl_PlanAccionRevision ON Tbl_ActaRevision.PK_Id_ActaRevision = Tbl_PlanAccionRevision.FK_Acta ON Tbl_Sede.Pk_Id_Sede = Tbl_ActaRevision.FK_Sede ON 
                         Tbl_ActividadesActosInseguros.FK_Id_Reportes = Tbl_Reportes.Pk_Id_Reportes
WHERE        (Tbl_Empresa.Razon_Social IS NOT NULL) 
GROUP BY Tbl_Empresa.Razon_Social, Tbl_Modulos_Plan_Accion.Modulo, Tbl_Modulos_Plan_Accion.Actividad, Tbl_Empresa.Nit_Empresa, Tbl_Acciones.Estado</CommandText>
      </Query>
