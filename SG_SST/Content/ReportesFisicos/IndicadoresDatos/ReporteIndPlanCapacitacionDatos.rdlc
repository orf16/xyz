  <DataSets>
    <DataSet Name="DataSetTotal">
      <Query>
        <DataSourceName>DataSourceSGSSTCERT</DataSourceName>
   
        <CommandText>SELECT        Tbl_PlanCapacitacion.pk_id_plan_capacitacion, YEAR(CONVERT(date, Tbl_PlanCapacitacion.fecha_programada, 103)) AS Anio, MONTH(CONVERT(date, 
                         Tbl_PlanCapacitacion.fecha_programada, 103)) AS Mes, Tbl_PlanCapacitacion.NitEmpresa, 
                         SUM(CASE WHEN Tbl_PlanCapacitacion_Asignaciones.asistencia = 1 THEN 1 ELSE 0 END) AS asistencia, Tbl_PlanCapacitacion.tema, 
                         Tbl_PlanCapacitacion.fecha_programada
FROM            Tbl_PlanCapacitacion LEFT OUTER JOIN
                         Tbl_PlanCapacitacion_Asignaciones ON Tbl_PlanCapacitacion.pk_id_plan_capacitacion = Tbl_PlanCapacitacion_Asignaciones.fk_id_plan_capacitacion

GROUP BY Tbl_PlanCapacitacion.pk_id_plan_capacitacion, YEAR(CONVERT(date, Tbl_PlanCapacitacion.fecha_programada, 103)), MONTH(CONVERT(date, 
                         Tbl_PlanCapacitacion.fecha_programada, 103)), Tbl_PlanCapacitacion.NitEmpresa, Tbl_PlanCapacitacion.tema, Tbl_PlanCapacitacion.fecha_programada</CommandText>
      </Query>
