          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Detalle">
          <DataField>Detalle</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_HHT">
          <DataField>Total_HHT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Mes">
          <DataField>Mes</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DiasAusenciaFormateados">
