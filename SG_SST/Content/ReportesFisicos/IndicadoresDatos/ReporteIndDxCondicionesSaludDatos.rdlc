  <DataSets>
    <DataSet Name="DataSetTotalDx">
      <Query>
        <DataSourceName>DataSourceSGSSTCERT</DataSourceName>
        <CommandText>SELECT      Tbl_Dx_Condiciones_De_Salud.Pk_DxCondicionesDeSalud, 'Sintomatología' as TipoAnormalidad, Tbl_Empresa.Nit_Empresa, Tbl_Empresa.Razon_Social, Tbl_Sede.Pk_Id_Sede, Tbl_Sede.Nombre_Sede, 
            Tbl_Proceso.Pk_Id_Proceso, Tbl_Proceso.Descripcion_Proceso, Tbl_Dx_Condiciones_De_Salud.Trabajadores_Lugar, Tbl_Dx_Condiciones_De_Salud.vigencia,
            Tbl_Sintomatologia_Dx.Sintomatologia AS DescripcionAnormalidad, Tbl_Sintomatologia_Dx.Trabajadores_Sintomatologia AS NumTrabajadoresAnormalidad
FROM            Tbl_Dx_Condiciones_De_Salud INNER JOIN
                         Tbl_Sintomatologia_Dx ON Tbl_Dx_Condiciones_De_Salud.Pk_DxCondicionesDeSalud = Tbl_Sintomatologia_Dx.FK_DxCondicionesDeSalud INNER JOIN
                         Tbl_Proceso ON Tbl_Dx_Condiciones_De_Salud.FK_Proceso = Tbl_Proceso.Pk_Id_Proceso INNER JOIN
                         Tbl_Sede ON Tbl_Dx_Condiciones_De_Salud.FK_Sede = Tbl_Sede.Pk_Id_Sede INNER JOIN
                         Tbl_Empresa ON Tbl_Sede.Fk_Id_Empresa = Tbl_Empresa.Pk_Id_Empresa

UNION
SELECT      Tbl_Dx_Condiciones_De_Salud.Pk_DxCondicionesDeSalud, 'Pruebas Clínicas' as TipoAnormalidad, Tbl_Empresa.Nit_Empresa, Tbl_Empresa.Razon_Social, Tbl_Sede.Pk_Id_Sede, Tbl_Sede.Nombre_Sede, 
			Tbl_Proceso.Pk_Id_Proceso, Tbl_Proceso.Descripcion_Proceso, Tbl_Dx_Condiciones_De_Salud.Trabajadores_Lugar, Tbl_Dx_Condiciones_De_Salud.vigencia,
			Tbl_Pruebas_Clinica_Dx.Prueba_Clinica AS DescripcionAnormalidad, Tbl_Pruebas_Clinica_Dx.Trabajadores_Con_Prueba AS NumTrabajadoresAnormalidad                                                 
FROM            Tbl_Dx_Condiciones_De_Salud INNER JOIN
                         Tbl_Pruebas_Clinica_Dx ON Tbl_Dx_Condiciones_De_Salud.Pk_DxCondicionesDeSalud = Tbl_Pruebas_Clinica_Dx.FK_DxCondicionesDeSalud INNER JOIN
                         Tbl_Proceso ON Tbl_Dx_Condiciones_De_Salud.FK_Proceso = Tbl_Proceso.Pk_Id_Proceso INNER JOIN
                         Tbl_Sede ON Tbl_Dx_Condiciones_De_Salud.FK_Sede = Tbl_Sede.Pk_Id_Sede INNER JOIN
                         Tbl_Empresa ON Tbl_Sede.Fk_Id_Empresa = Tbl_Empresa.Pk_Id_Empresa

UNION
SELECT      Tbl_Dx_Condiciones_De_Salud.Pk_DxCondicionesDeSalud, 'Pruebas Paraclínicas' as TipoAnormalidad, Tbl_Empresa.Nit_Empresa, Tbl_Empresa.Razon_Social, Tbl_Sede.Pk_Id_Sede, Tbl_Sede.Nombre_Sede, 
			Tbl_Proceso.Pk_Id_Proceso, Tbl_Proceso.Descripcion_Proceso, Tbl_Dx_Condiciones_De_Salud.Trabajadores_Lugar, Tbl_Dx_Condiciones_De_Salud.vigencia, 
			Tbl_Pruebas_P_Clinica_Dx.Prueba_P_Clinica AS DescripcionAnormalidad, Tbl_Pruebas_P_Clinica_Dx.Trabajadores_Con_Prueba_P AS NumTrabajadoresAnormalidad                                                  
FROM            Tbl_Dx_Condiciones_De_Salud INNER JOIN
                         Tbl_Pruebas_P_Clinica_Dx ON Tbl_Dx_Condiciones_De_Salud.Pk_DxCondicionesDeSalud = Tbl_Pruebas_P_Clinica_Dx.FK_DxCondicionesDeSalud INNER JOIN
                         Tbl_Proceso ON Tbl_Dx_Condiciones_De_Salud.FK_Proceso = Tbl_Proceso.Pk_Id_Proceso INNER JOIN
                         Tbl_Sede ON Tbl_Dx_Condiciones_De_Salud.FK_Sede = Tbl_Sede.Pk_Id_Sede INNER JOIN
                         Tbl_Empresa ON Tbl_Sede.Fk_Id_Empresa = Tbl_Empresa.Pk_Id_Empresa

UNION
SELECT      Tbl_Dx_Condiciones_De_Salud.Pk_DxCondicionesDeSalud, 'Diagnóstico CIE10' as TipoAnormalidad, Tbl_Empresa.Nit_Empresa, Tbl_Empresa.Razon_Social, Tbl_Sede.Pk_Id_Sede, Tbl_Sede.Nombre_Sede, 
