  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>dbpositiva</DataSourceName>
        <CommandText>SELECT        dbo.Tbl_Empresa.Nit_Empresa, dbo.Tbl_Empresa.Razon_Social, dbo.Tbl_ComunicacionesExternas.FechaCreacion, 
                         dbo.Tbl_ComunicacionesExternas.EstadoComunicado, dbo.Tbl_ComunicacionesExternas.Asunto
FROM            dbo.Tbl_ComunicacionesExternas INNER JOIN
                         dbo.Tbl_Empresa ON dbo.Tbl_ComunicacionesExternas.NitEmpresa = dbo.Tbl_Empresa.Nit_Empresa</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Nit_Empresa">
          <DataField>Nit_Empresa</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Razon_Social">
          <DataField>Razon_Social</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FechaCreacion">
          <DataField>FechaCreacion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EstadoComunicado">
          <DataField>EstadoComunicado</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Asunto">
          <DataField>Asunto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>dbpositiva</DataSourceName>
   
        <CommandText>SELECT        COUNT(*) AS ID, Tbl_ComunicacionesExternas.EstadoComunicado AS 'Estado Comunicado'
FROM            Tbl_ComunicacionesExternas INNER JOIN
                         Tbl_Empresa ON Tbl_ComunicacionesExternas.NitEmpresa = Tbl_Empresa.Nit_Empresa

GROUP BY Tbl_ComunicacionesExternas.EstadoComunicado</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Estado_Comunicado">
          <DataField>Estado Comunicado</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
