  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>db</DataSourceName>
        
        <CommandText>SELECT        Tbl_Empresa.Razon_Social AS EMPRESA, Tbl_Rol.Descripcion AS ROL, Tbl_Cargo.Nombre_Cargo AS CARGO, COUNT(Tbl_UsuarioRol.Pk_Id_UsuarioRol) 
                         AS NUMERO_DE_PERSONAS
FROM            Tbl_Empresa INNER JOIN
                         Tbl_Rol ON Tbl_Empresa.Pk_Id_Empresa = Tbl_Rol.Fk_Id_Empresa INNER JOIN
                         Tbl_Cargo_Por_Rol INNER JOIN
                         Tbl_Cargo ON Tbl_Cargo_Por_Rol.Fk_Id_Cargo = Tbl_Cargo.Pk_Id_Cargo ON Tbl_Rol.Pk_Id_Rol = Tbl_Cargo_Por_Rol.Fk_Id_Rol INNER JOIN
                         Tbl_UsuarioRol ON Tbl_Rol.Pk_Id_Rol = Tbl_UsuarioRol.Fk_Id_Rol

GROUP BY Tbl_Empresa.Razon_Social, Tbl_Rol.Descripcion, Tbl_Cargo.Nombre_Cargo</CommandText>
      </Query>
      <Fields>
        <Field Name="EMPRESA">
          <DataField>EMPRESA</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ROL">
